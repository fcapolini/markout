{
  "name": "markout",
  "version": "1.0.0",
  "description": "A TypeScript Node.js Express.js project",
  "main": "dist/index.js",
  "scripts": {
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "build": "node scripts/build-server.mjs && node scripts/build-client.mjs",
    "dev": "nodemon --exec ts-node src/index.ts",
    "start": "node dist/index.js",
    "start:prod": "pm2 start ecosystem.config.js --env production",
    "start:dev": "pm2 start ecosystem.config.js --env development",
    "stop": "pm2 stop ecosystem.config.js",
    "restart": "pm2 restart ecosystem.config.js",
    "reload": "pm2 reload ecosystem.config.js",
    "delete": "pm2 delete ecosystem.config.js",
    "logs": "pm2 logs",
    "monit": "pm2 monit",
    "watch": "tsc --watch",
    "clean": "rm -rf dist",
    "test:watch": "vitest --watch",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "keywords": [
    "typescript",
    "node",
    "express"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/compression": "^1.8.1",
    "commander": "^14.0.1",
    "compression": "^1.8.1",
    "escodegen": "2.1.0",
    "estraverse": "5.3.0",
    "express": "5.1.0",
    "express-rate-limit": "8.1.0",
    "pm2": "6.0.13"
  },
  "devDependencies": {
    "@types/escodegen": "0.0.10",
    "@types/estraverse": "5.1.7",
    "@types/express": "5.0.3",
    "@types/jsdom": "27.0.0",
    "@types/node": "24.7.2",
    "@types/supertest": "6.0.3",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "3.2.4",
    "esbuild": "^0.25.10",
    "happy-dom": "^20.0.0",
    "jsdom": "27.0.0",
    "nodemon": "3.1.10",
    "prettier": "3.6.2",
    "supertest": "7.1.4",
    "ts-node": "10.9.2",
    "typescript": "5.9.3",
    "vitest": "3.2.4"
  }
}
